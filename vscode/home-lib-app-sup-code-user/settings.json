{
  "cSpell.userWords": [
    "Alderaan",
    "Cryptographically",
    "Heroku",
    "architecting",
    "blockchain",
    "orderbook",
    "sharded",
    "sharex"
  ],
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontFamily": "Fira Code, Inconsolata, Hack, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "elixirLS.fetchDeps": false,
  "eslint.packageManager": "yarn",
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.autoSave": "onFocusChange",
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.ignoreMissingGitWarning": true,
  "gitlens.advanced.messages": { "suppressShowKeyBindingsNotice": true },
  "markdown.extension.orderedList.autoRenumber": false,
  "search.location": "panel",
  "window.newWindowDimensions": "maximized",
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "all",
  "window.zoomLevel": 2,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "dark+(elixir)",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "[ruby]": { "editor.formatOnSave": true },
  "ruby.format": "rubocop",
"editor.formatOnSaveTimeout": 1500,
"ruby.codeCompletion": "rcodetools",
"ruby.intellisense": "rubyLocate",
"ruby.useLanguageServer": true
}
